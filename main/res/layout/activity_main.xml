<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/idControl"
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_marginStart="178dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="176dp"
        android:background="#051250"
        android:text="CONTROL VEHICULO AUTONOMO"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/idIncrementar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="356dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="324dp"
        android:backgroundTint="#051250"
        android:foregroundTint="#FF0000"
        android:shadowColor="#DA0B0B"
        android:text="@string/mÃ¡s"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/idCantidad"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/idRestar"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="356dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="324dp"
        android:backgroundTint="#051250"
        android:text="@string/less"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idCantidad"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/idAceptar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="168dp"
        android:layout_marginTop="94dp"
        android:layout_marginEnd="171dp"
        android:layout_marginBottom="18dp"
        android:backgroundTint="#051250"
        android:text="@string/aceptar"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/texto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idCantidad" />

    <TextView
        android:id="@+id/idCantidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="360dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="32dp"
        android:textColor="#7E7A7B"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/idAceptar"
        app:layout_constraintEnd_toStartOf="@+id/idRestar"
        app:layout_constraintStart_toEndOf="@+id/idIncrementar"
        app:layout_constraintTop_toBottomOf="@+id/idControl"
        app:layout_constraintVertical_bias="0.48000002" />

    <TextView
        android:id="@+id/nombre"
        android:layout_width="59dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="164dp"
        android:textColor="#7E7A7B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/materia"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/materia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="69dp"
        android:textColor="#7E7A7B"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/idIncrementar"
        app:layout_constraintStart_toStartOf="@+id/textView8" />

    <TextView
        android:id="@+id/apellido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="80dp"
        android:textColor="#7E7A7B"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/nombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nombre"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/numero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="68dp"
        android:textColor="#7E7A7B"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/idRestar"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="67dp"
        android:layout_marginBottom="10dp"
        android:text="PEDIDO"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#051250"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/nombre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idControl"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="66dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="11dp"
        android:background="#00CDDC39"
        android:text="REFERENCIA"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#051250"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/apellido"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idControl"
        app:layout_constraintVertical_bias="0.142" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="56dp"
        android:layout_marginBottom="8dp"
        android:text="POSICION"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#051250"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/materia"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nombre"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="113dp"
        android:layout_height="31dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="52dp"
        android:text="CANASTA"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#051250"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apellido" />

    <TextView
        android:id="@+id/texto"
        android:layout_width="158dp"
        android:layout_height="32dp"
        android:layout_marginStart="182dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="184dp"
        android:layout_marginBottom="204dp"
        android:text="-"
        android:textColor="#7E7A7B"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idAceptar" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginBottom="86dp"
        android:backgroundTint="#051250"
        android:text="HOME"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/stop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="84dp"
        android:backgroundTint="#051250"
        android:text="STOP"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/exit"
        app:layout_constraintStart_toEndOf="@+id/home" />

    <Button
        android:id="@+id/exit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="86dp"
        android:backgroundTint="#051250"
        android:text="SALIR"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/stop" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginEnd="164dp"
        android:layout_marginBottom="26dp"
        android:text="CANTIDAD"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#051250"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/idCantidad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>